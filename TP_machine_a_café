int boutonPin[3]={7,6,5};
int ledPin[3]={10,9,8};
int stock[3]={10,5,5};
String commande[3]={"café","thé","chocolat chaud"};
void setup()
{
  pinMode(7, INPUT);
  pinMode(10, OUTPUT);
  pinMode(6, INPUT);
  pinMode(9, OUTPUT);
  pinMode(5, INPUT);
  pinMode(8, OUTPUT);
}
int remplir=1;
void loop()
{
  digitalWrite(10,HIGH);
  digitalWrite(9,HIGH);
  digitalWrite(8,HIGH);
  
  for (int i=0;i<3;i++)
  {  
    if(stock[i]==5)
    {
      digitalWrite(ledPin[i],LOW);
    }
  }
    
}
